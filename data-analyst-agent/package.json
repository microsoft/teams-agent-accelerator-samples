{
  "name": "data-analyst-agent",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "npx rimraf ./dist",
    "build": "npx tsup",
    "start": "node .",
    "dev": "npx nodemon -w \"./src/**\" -e ts --exec \"node -r ts-node/register -r dotenv/config ./src/index.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "eval:sql": "ts-node -r dotenv/config evals/sql-expert.ts",
    "eval:ac": "ts-node -r dotenv/config evals/ac-expert.ts"
  },
  "dependencies": {
    "@teams.sdk/ai": "^0.1.26",
    "@teams.sdk/api": "^0.1.26",
    "@teams.sdk/apps": "^0.1.26",
    "@teams.sdk/cards": "^0.1.26",
    "@teams.sdk/common": "^0.1.26",
    "@teams.sdk/dev": "^0.1.26",
    "@teams.sdk/devtools": "^0.1.26",
    "@teams.sdk/graph": "^0.1.26",
    "@teams.sdk/openai": "^0.1.26",
    "ajv": "^8.17.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.4",
    "prettier": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "typescript": "^5.4.5"
  }
}
